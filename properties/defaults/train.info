; Training configuration
;

train
{
	C 0.1	; Regularization factor
	iter
	{
		start 0		; Starting iteration
		end 1000	; End iteration
	}
	rate
	{
		alpha 0.001	; Step size
		beta1 0.9   ; Exponential decay rate for 1st moment estimate
		beta2 0.999 ; Exponential decay rate for 2nd moment estimate
		eps 10e-8   ; Small epsilon value to prevent division-by-zero errors
	}
}
